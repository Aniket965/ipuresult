    <!-- Add your site or application content here -->
    <div class='result-page'>
        <div id='grad2'>
            <div id='grad1'>
                <div>
                    <div class='first' style="cursor: pointer;" onclick="back()">IPU RESULT</div>
                    <div class='second'>
                        <%= total.toFixed(2) %>%(T)</div>
                    <div class='third'>
                        <%= credit.toFixed(2) %>%(C)</div>
                </div>
            </div>

            <h1 id='head1' style="text-align: left; font-weight: 2; margin-left:20px ">
                <%= name %>
            </h1>
            <h2 id='head2' style="text-align: left; font-weight: 2; margin-left:20px ">
                <%= rollnumber %>
            </h2>
            <% for(var i =0 ; i < result.length;i++){%>
                <div class='w3-container'>
                    <div class='w3-panel w3-card'>

                        <%if (rollnumber.indexOf("024") === 6) { %>
                            <div class="subject-name">
                                <%= codes[241 + result[i].id.substr(3,2)] %>
                            </div>


                            <%} else{%>
                                <div class="subject-name">

                                </div>
                                <%}%>
                                    <div class='marks'>
                                        <div class='rect'>
                                            <div class='circle'>
                                                <div>
                                                    <i class='material-icons' style="color:black;" >equalizer</i>
                                                </div>
                                                  <div class='wave -three'></div>
                                            </div>
                                        </div>
                                        <div class="w3card-result-id">

                                            <div class='grid3'>
                                                <%= result[i].id %>
                                            </div>
                                        </div>
                                        <div class="w3card-result" >
                                            <div class='grid2'>EX
                                                <%= result[i].external_mark %>
                                            </div>
                                            <div class='grid3'>IN
                                                <%= result[i].internal_mark %>
                                            </div>
                                        </div>

                                        <div class='grid4'>
                                            <%= result[i].total %>
                                        </div>

                                    </div>
                    </div>
                </div>
                <%}%>
        </div>
        <div style="padding:2rem;"></div>
    </div>


